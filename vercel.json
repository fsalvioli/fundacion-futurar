{
    "builds": [
      {
        "src": "news_page/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "staticfiles/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "media/$1"
      },
      {
        "src": "/favicon.ico",
        "dest": "staticfiles/favicon.ico"
      },
      {
        "src": "/(.*)",
        "dest": "news_page/wsgi.py"
      }
    ]
  }
  